<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN"
        "https://gwt.googlesource.com/gwt/+/master/./distro-source/core/src/gwt-module.dtd">
<!-- To compile to Gwt, you need to reference this file in your master .gwt.xml
     file (usually called GdxDefinition.gwt.xml) as follows:
     <inherits name='squidlib-util' />
  -->
<module>
    <source path="squidpony"/>
    <inherits name='regexodus'/>
    <extend-configuration-property name="gdx.reflect.include" value="squidpony" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidmath" />
    <extend-configuration-property name="gdx.reflect.exclude" value="squidpony.squidmath.MersenneTwister" />
    <extend-configuration-property name="gdx.reflect.exclude" value="squidpony.squidmath.Elias" />
    <extend-configuration-property name="gdx.reflect.exclude" value="squidpony.squidmath.ChaosRNG" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidgrid" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidgrid.mapping" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidgrid.mapping.styled" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidai.ZOI" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidai.AimLimit" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidai.Reach" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidai.Technique" />
    <extend-configuration-property name="gdx.reflect.include" value="squidpony.squidai.Threat" />
</module>
